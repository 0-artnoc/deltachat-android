    <string name="Intro1Headline">Delta Chat</string>
    <string name="Intro1Message">La mensajería de <b>mayor alcance</b> del mundo. <b>Libre</b> y <b>segura</b>.</string>

    <string name="Intro2Headline">Independiente</string>
    <string name="Intro2Message"><b>Sin dependencia</b> de ordenadores o servidores ajenos. Usa solo tu servidor de correo.</string>

    <string name="Intro3Headline">Rápido</string>
    <string name="Intro3Message"><b>Mensajes automáticos</b> en segundos.<br/>Interfaz rápida.</string>

    <string name="Intro4Headline">Potente</string>
    <string name="Intro4Message">Conversaciones, imágenes, vídeos, audiomensajes <b>ilimitados</b> y más. Capacidad multicliente</string>

    <string name="Intro5Headline">Libre</string>
    <string name="Intro5Message"><b>Delta Chat</b> será siempre libre.<br/>Fuente abierta. Sin anuncios. Sin suscripción. Sin atarse a un proveedor.</string>

    <string name="Intro6Headline">Seguro</string>
    <string name="Intro6Message"><b>Cifrado</b> con algoritmos conocidos. Los mensajes permanecen en tus servidores.</string>

    <string name="Intro7Headline">Confiable</string>


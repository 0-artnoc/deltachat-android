    <string name="Intro1Headline">Delta Chat</string>
    <string name="Intro1Message">L\'app de missatgeria  <![CDATA[<b> de més abast</b>]]> al món. <![CDATA[<b>lliure</b>]]> i <![CDATA[<b>independent</b>]]>.</string>

    <string name="Intro2Headline">Independent</string>
    <string name="Intro2Message"><![CDATA[<b>No hi ha un servidor Delta Chat.</b>]]> L\'app només fa servir el teu servidor de correu electrònic.</string>

    <string name="Intro3Headline">Ràpid</string>
    <string name="Intro3Message"><![CDATA[<b>Envia missatges</b>]]> en segons.<![CDATA[<br/>]]>Interfície ràpida.</string>

    <string name="Intro4Headline">Potent</string>
    <string name="Intro4Message"><![CDATA[<b>Il·limitats</b>]]> xats, imatges, vídeos, àudios missatges i més. Capacitat multi-client.</string>

    <string name="Intro5Headline">De franc</string>
    <string name="Intro5Message"><![CDATA[<b>Delta Chat</b>]]> serà de franc sempre.<![CDATA[<br/>]]>Codi obert. Sense publicitat. Sense subscripció. Sense exclusivitat de proveïdor.</string>

    <string name="Intro6Headline">Segur</string>
    <string name="Intro6Message"><![CDATA[<b>Encriptat</b>]]> amb tots els algoritmes habituals. Els missatges romanen als teus servidors.</string>


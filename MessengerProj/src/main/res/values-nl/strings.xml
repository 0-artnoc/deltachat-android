    <!--Intro view-->
    <string name="Intro1Headline">Delta Chat</string>
    <string name="Intro1Message">De berichtenapp met het <![CDATA[<b>grootste bereik</b>]]> van de hele wereld. <![CDATA[<br/><b>Gratis</b>]]> en <![CDATA[<b>onafhankelijk</b>]]>.</string>

    <string name="Intro2Headline">Onafhankelijk</string>
    <string name="Intro2Message"><![CDATA[<b>Er is geen globale Delta Chat-server.</b>]]> De app gebruikt alleen je eigen mailserver.</string>

    <string name="Intro3Headline">Snel</string>
    <string name="Intro3Message"><![CDATA[<b>Push-berichten</b>]]> in seconden.<![CDATA[<br/>]]>Snelle gebruikersomgeving.</string>

    <string name="Intro4Headline">Krachtig</string>
    <string name="Intro4Message"><![CDATA[<b>Onbeperkte</b>]]> chats, afbeeldingen, filmpjes, audioberichten en meer. Werkt samen met andere clients.</string>

    <string name="Intro5Headline">Gratis</string>
    <string name="Intro5Message"><![CDATA[<b>Delta Chat</b>]]> is en blijft gratis.<![CDATA[<br/>]]>Open bron-software, geen reclame, geen abonnement en onafhankelijk van leveranciers.</string>

    <string name="Intro6Headline">Versleuteld</string>
    <string name="Intro6Message"><![CDATA[<b>Versleuteld</b>]]> met alle beproefde algoritmes. Berichten blijven op je eigen server.</string>


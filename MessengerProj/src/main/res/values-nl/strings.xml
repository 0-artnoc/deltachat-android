    <string name="Intro1Headline">Delta Chat</string>
    <string name="Intro1Message">De berichtenapp met het <![CDATA[<b>grootste bereik</b>]]> in de hele wereld. <![CDATA[<br/><b>gratis</b>]]> en <![CDATA[<b>veilig</b>]]>.</string>

    <string name="Intro2Headline">Onafhankelijk</string>
    <string name="Intro2Message"><![CDATA[<b>Geen vereisten</b>]]> van andere computers of diensten. De app gebruikt alleen je mailserver.</string>

    <string name="Intro3Headline">Snel</string>
    <string name="Intro3Message"><![CDATA[<b>Push-berichten</b>]]> in seconden.<![CDATA[<br/>]]>Snelle interface.</string>

    <string name="Intro4Headline">Krachtig</string>
    <string name="Intro4Message"><![CDATA[<b>Onbeperkte</b>]]> chats, afbeeldigen, filmpjes, audio berichten en meer. Werkt samen met andere clients.</string>

    <string name="Intro5Headline">Gratis</string>
    <string name="Intro5Message"><![CDATA[<b>Delta Chat</b>]]> is voor altijd gratis..<![CDATA[<br/>]]>Open source, geen reclame, geen abonnement, geen vendor lock-in.</string>

    <string name="Intro6Headline">Veilig</string>
    <string name="Intro6Message"><![CDATA[<b>Beveiligd</b>]]> met beproefde algoritmes. Berichten blijven op je server.</string>

